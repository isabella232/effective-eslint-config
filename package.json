{
  "name": "@effective/eslint-config",
  "version": "2.0.0",
  "description": "ESLint Configuration for Effective Project",
  "author": {
    "name": "Sebastian Software",
    "email": "s.werner@sebastian-software.de",
    "url": "https://www.sebastian-software.de"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/effective-eslint-config.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "release": "git push && release-it --github.release --npm.publish --non-interactive",
    "release:minor": "git push && release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "git push && release-it --github.release --npm.publish --non-interactive --increment major",
    "build": "preppy",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "prelint": "npm run build",
    "lint": "run-s \"lint:* {@}\" --",
    "lint:script": "eslint --fix '**/*.{js,jsx,ts,tsx,mjs}'",
    "lint:types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,mjs,json,md}' && eslint --fix '**/*.{js,jsx,ts,tsx,mjs}'",
    "check:conflicts": "eslint --print-config . | eslint-config-prettier-check",
    "pretest": "npm run build",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "eslint": ">=6.8.0",
    "prettier": ">=2.0.2",
    "typescript": ">=3.8.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-shopify-lean": "^1.1.0",
    "eslint-plugin-unicorn": "^17.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/eslint": "^6.1.8",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "eslint": "^6.7.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "preppy": "^9.3.1",
    "prettier": "^1.18.2",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.6.3"
  }
}
