{
  "name": "@effective/eslint-config",
  "version": "2.6.3",
  "description": "ESLint Configuration for Effective Project",
  "author": {
    "name": "Sebastian Software",
    "email": "s.werner@sebastian-software.de",
    "url": "https://www.sebastian-software.de"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/effective-eslint-config.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "release": "git push && release-it --ci patch",
    "release:minor": "git push && release-it --ci minor",
    "release:major": "git push && release-it --ci major",
    "build": "preppy",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "prelint": "npm run build",
    "lint": "run-s \"lint:* {@}\" --",
    "lint:script": "eslint --fix '**/*.{js,jsx,ts,tsx,mjs}'",
    "lint:types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,mjs,json,md}' && eslint --fix '**/*.{js,jsx,ts,tsx,mjs}'",
    "check:conflicts": "eslint --print-config src/index.js | eslint-config-prettier-check || exit 0",
    "pretest": "npm run build",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "eslint": ">=7.1.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "app-root-path": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsdoc": "^26.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-shopify-lean": "^1.1.2",
    "eslint-plugin-unicorn": "^20.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "preppy": "^10.4.1",
    "prettier": ">=2.0.5",
    "react": "^16.13.1",
    "release-it": "^13.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  }
}
