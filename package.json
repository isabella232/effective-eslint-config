{
  "name": "@effective/eslint-config",
  "version": "0.0.1",
  "description": "ESLint Configuration for Effective Project",
  "author": "Sebastian Werner <s.werner@sebastian-software.de>",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "preppy",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "prelint": "npm run build",
    "lint": "run-s \"lint:* {@}\" --",
    "lint:style": "prettier --check '**/*.{js,jsx,json,md,ts,tsx}'",
    "lint:script": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:types": "tsc --noEmit",
    "check:conflicts": "eslint --print-config . | eslint-config-prettier-check",
    "pretest": "npm run build",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "eslint": ">= 5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "confusing-browser-globals": "1.0.7",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/eslint": "4.16.6",
    "@types/jest": "24.0.13",
    "@types/node": "10.14.8",
    "@types/prettier": "1.16.4",
    "@types/semver": "6.0.0",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "5.16.0",
    "husky": "2.4.0",
    "jest": "24.8.0",
    "lint-staged": "8.2.0",
    "npm-run-all": "^4.1.5",
    "preppy": "8.1.3",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "rimraf": "2.6.3",
    "typescript": "3.5.1"
  }
}
